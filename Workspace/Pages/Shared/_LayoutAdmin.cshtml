<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <title>@ViewData["Title"]</title>
    <meta content="" name="description">
    <meta content="" name="keywords">

    <!--Login StyleSheet-->
    <link href="~/css/Login.css" rel="stylesheet">

    <!--Products StyleSheet-->
    <link href="~/css/Products.css" rel="stylesheet">


    <!-- Favicons -->
    @* <link href="assets/img/favicon.png" rel="icon">
    <link href="assets/img/apple-touch-icon.png" rel="apple-touch-icon">*@

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/vendor/animate.css/animate.min.css" rel="stylesheet">
    <link href="~/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="~/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

    <!--Web Icon-->
    <link href="~/Icon.ico" rel="icon" />

    <!-- Template Main CSS File -->
    <link href="~/css/style.css" rel="stylesheet">

    <style>
        #navbar #ListNav .linkHead:hover {
            color: #FFD95A !important;
        }
    </style>

    <!--For The Charts-->
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <script src="~/js/html2pdf.bundle.min.js"></script>


</head>


<body>
    <header id="header" class="d-flex align-items-center">
        <div class="container d-flex align-items-center">

            <h1 class="logo me-auto"><a asp-page="/Index">Creative Zone</a></h1>


            <!-- Uncomment below if you prefer to use an image logo -->
            <!-- <a href="index.html" class="logo me-auto"><img src="assets/img/logo.png" alt="" class="img-fluid"></a>-->

            <nav id="navbar" class="navbar">
                <ul id="ListNav">

                    <li><a class="nav-link scrollto active linkHead" style="color:white;" asp-page="/Admin/Welcome">Home</a></li>
                    <li><a class="nav-link scrollto linkHead" href="/Index#services" style="color:white;">Services</a></li>
                    <li><a class="nav-link scrollto linkHead" href="/Index#about" style="color:white;">About</a></li>
                    <li class="dropdown">
                        <a href="#"><span class="linkHead" style="color:white;">Explore</span> <i class="bi bi-chevron-down"></i></a>
                        <ul>
                            <li><a asp-page="/MyTasks/Events">Events</a></li>
                            <li><a asp-page="/MyTasks/Rooms">Rooms</a></li>
                            <li><a asp-page="/Products/Products">Products</a></li>
                            <li><a asp-page="/MyTasks/Subscription">Subscriptions</a></li>
                            <li><a asp-page="/employees_pages/employees_list">Employees</a></li>
                            <li><a asp-page="/Customer_pages/customer_list">Customers</a></li>
                        </ul>
                    </li>
                    <li><a class="nav-link scrollto linkHead" href="/Index#contact" style="color:white;">Contact</a></li>
                    <li><a class="nav-link scrollto linkHead" asp-page="/LogIn/SignUp" style="color:white;">Sign Up</a></li>
                    <li><a class="getstarted scrollto" href="#about" style="margin-left: 40px;">Get Started</a></li>
                    <li><a class="nav-link scrollto" asp-page=""><i id="profileIcon" class="bi bi-person-circle fa-lg" style="font-size: 30px;"></i></a></li>
                    <li><a class="nav-link scrollto" asp-page="/Index" asp-route-userType="0"><i class="bi bi-box-arrow-left" style="font-size: 30px;height: 30px;"></i></a></li>


                </ul>
                <i class="bi bi-list mobile-nav-toggle"></i>
            </nav><!-- .navbar -->

        </div>

        

    </header>

    <div class="container">
        @RenderBody()
    </div>

    <footer id="footer">
        <div class="container">
            <h3>Creative Zone</h3>
            <p>Where Inspiration Meets Productivity.</p>
            <div class="social-links">
                <a href="#" class="twitter"><i class="bx bxl-twitter"></i></a>
                <a href="#" class="facebook"><i class="bx bxl-facebook"></i></a>
                <a href="#" class="instagram"><i class="bx bxl-instagram"></i></a>
                <a href="#" class="google-plus"><i class="bx bxl-skype"></i></a>
                <a href="#" class="linkedin"><i class="bx bxl-linkedin"></i></a>
            </div>
            <div class="copyright">
                © Copyright <strong><span>Creative Zone</span></strong>. <span style="color:white;">All Rights Reserved</span>
            </div>
        </div>
    </footer>
    <a href="#" class="back-to-top d-flex align-items-center justify-content-center active"><i class="bi bi-arrow-up-short"></i></a>
    <script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/vendor/php-email-form/validate.js"></script>
    <script src="~/vendor/aos/aos.js"></script>
    <script src="~/vendor/purecounter/purecounter_vanilla.js"></script>




    <!--Home JS-->
    <script src="~/js/main.js"></script>
    <!--Products JS-->
    <script src="~/js/Products.js"></script>



    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>